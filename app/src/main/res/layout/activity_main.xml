<?xml version="1.0" encoding="utf-8"?>

    <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            tools:context=".MainActivity">

    <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="134dp"
            app:srcCompat="@drawable/arhcer_title" android:contentDescription="@string/todo"/>

    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

        <ImageButton
                android:id="@+id/archer_1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:maxWidth="150dp"
                android:maxHeight="150dp"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/archer_1" tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck"
                android:contentDescription="@string/todo"/>

        <ImageButton
                android:id="@+id/lana_1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:maxWidth="150dp"
                android:maxHeight="150dp"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/lana_1" tools:ignore="SpeakableTextPresentCheck"
                android:contentDescription="@string/todo"/>

    </LinearLayout>

        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

            <ImageButton
                    android:id="@+id/cyril_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:maxWidth="150dp"
                    android:maxHeight="150dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/cyril_1" tools:ignore="SpeakableTextPresentCheck"
                    android:contentDescription="@string/todo"/>

            <ImageButton
                    android:id="@+id/pam_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:maxWidth="150dp"
                    android:maxHeight="150dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/pam_1" tools:ignore="SpeakableTextPresentCheck"
                    android:contentDescription="@string/todo"/>

        </LinearLayout>

        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

            <ImageButton
                    android:id="@+id/krieger_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:maxWidth="150dp"
                    android:maxHeight="150dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/krieger_1" tools:ignore="SpeakableTextPresentCheck"
                    android:contentDescription="@string/todo"/>

            <ImageButton
                    android:id="@+id/cheryl_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:maxWidth="150dp"
                    android:maxHeight="150dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/cheryl_1" tools:ignore="SpeakableTextPresentCheck"
                    android:contentDescription="@string/todo"/>

        </LinearLayout>

        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

            <ImageButton
                    android:id="@+id/ray_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:maxWidth="150dp"
                    android:maxHeight="150dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/ray_1" tools:ignore="SpeakableTextPresentCheck"
                    android:contentDescription="@string/todo"/>

            <ImageButton
                    android:id="@+id/mallory_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:adjustViewBounds="true"
                    android:maxWidth="150dp"
                    android:maxHeight="150dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/mallory_1"
                    tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck"
                    android:contentDescription="@string/todo"/>

        </LinearLayout>

    </LinearLayout>

